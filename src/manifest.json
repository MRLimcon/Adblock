{
  "name": "Tatu Blocker",
  "version": "1.0.0",
  "description": "Bloqueador minimalista de an√∫ncios",
  "permissions": [
      "cookies",
      "webRequest", 
      "webRequestBlocking", 
      "<all_urls>",
      "privacy",
      "bookmarks",
      "storage",
      "tabs", 
      "webNavigation"
    ],
  "background": {
    "scripts": [
      "blocked_domains.js",
      "blocked_parts.js",
      "blocked_cosmetic.js",
      "blockedjs.js",
      "background.js", 
      "script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
        "19": "tatuBlocker.png",
        "38": "tatuBlocker.png"
    },
    "default_title": "Tatu Blocker",
    "default_popup": "popup.html",
    "default_script": "popup.js"
},
  "manifest_version": 2
}
