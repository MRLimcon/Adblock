{
  "name": "Tatu Blocker",
  "version": "0.1",
  "description": "Bloqueador de an√∫ncios experimental",
  "permissions": ["webRequest", "webRequestBlocking", "http://*/*", "https://*/*", "tabs", "webNavigation", "notifications"],
  "background": {
    "scripts": [
      "blocked_domains.js",
      "blocked_parts.js", 
      "background.js", 
      "blocked_cosmetic.js",
      "blocked_js.js"
    ],
    "content_scripts": [{
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["script.js"]
      }],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
